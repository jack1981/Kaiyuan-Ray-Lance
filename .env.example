# Local defaults
PIPELINE_CONFIG=example/read_write.yaml
RAY_MODE=local
RAY_ADDRESS=

# Kubernetes (kind + KubeRay) defaults
KIND_CLUSTER_NAME=kaiyuan-ray
K8S_NAMESPACE=kaiyuan-ray
K8S_SERVICE_ACCOUNT=ray-job-runner
RAY_K8S_IMAGE=kaiyuan-ray-app:latest
RAY_JOB_IMAGE=kaiyuan-ray-app:latest
K8S_WAIT_TIMEOUT_SECONDS=3600
K8S_POLL_INTERVAL_SECONDS=5

# MinIO / S3A defaults for Ray-on-K8s
MINIO_ACCESS_KEY=minio
MINIO_SECRET_KEY=minio123
MINIO_BUCKET=kaiyuan-ray
K8S_DATA_BUCKET=kaiyuan-ray
MINIO_ENDPOINT=minio.kaiyuan-ray.svc.cluster.local:9000
MINIO_SYNC_ENDPOINT=host.docker.internal:30900
MINIO_SCHEME=http
AWS_ENDPOINT_URL=http://minio.kaiyuan-ray.svc.cluster.local:9000
AWS_REGION=us-east-1
LANCE_AWS_ALLOW_HTTP=1
